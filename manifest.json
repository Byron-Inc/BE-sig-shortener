{
    "name": "Signature Shortener",
    "description": "Shortens signature.",
    "version": "0.1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://tbgforums.com/*", "https://tbgforums.com/*"],
            "css": ["style.css"]
        }
    ],
    "action": {
        "default_icon": {
            "128": "logo.png"
        }
    }, 
    "icons": {
        "128": "logo.png"
    }
}